{
  "name": "server-v2",
  "version": "1.0.0",
  "main": "index.js",
  "author": "alpoxdev",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev nodemon ./src/index.ts -r tsconfig-paths/register 0.0.0.0 8080",
    "dev:prod": "cross-env NODE_ENV=prod nodemon ./src/index.ts -r tsconfig-paths/register 0.0.0.0 8080",
    "prod": "node dist/index.js",
    "build:stage": "cross-env NODE_ENV=stage tsc -p tsconfig.json",
    "build:prod": "cross-env NODE_ENV=prod tsc -p tsconfig.json",
    "image:prod": "rimraf dist && docker build -t alpox-server --no-cache ."
  },
  "dependencies": {
    "dotenv": "10.0.0",
    "fastify": "^3.17.0",
    "fastify-plugin": "3.0.0",
    "mariadb": "2.5.3",
    "nanoid": "3.1.23",
    "node-schedule": "2.0.0",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.6.2",
    "sequelize-typescript": "2.1.0",
    "telebot": "1.4.1"
  },
  "devDependencies": {
    "@types/node": "15.12.2",
    "@types/node-schedule": "1.3.1",
    "@types/telebot": "1.2.2",
    "@types/validator": "13.1.4",
    "cross-env": "7.0.3",
    "nodemon": "2.0.7",
    "tsc": "2.0.3",
    "tsconfig-paths": "3.9.0",
    "typescript": "^4.3.2"
  }
}
